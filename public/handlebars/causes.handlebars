<div id="causenav">
  <ul class="button-group">
    <li>
      <a href="#" class="small button round" {{action "showmap"}}>
        Map
      </a>
    </li>
    <li>
      <a href="#" class="small button round" {{action "showgrid"}}>
        Grid 
      </a>
    </li>

    <li>
    {{
      view Ember.Select 
      contentBinding="controller.cities"
      selectionBinding="controller.activeCity" 
    }}
    </li>
    <li>
    {{
      view Ember.Select 
      contentBinding="controller.types"
      selectionBinding="controller.activeType" 
    }}
      
    </li>
  </ul>
</div>

{{outlet}}
